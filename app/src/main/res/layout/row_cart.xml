<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="cartRowModel" type="com.test_.app.modules.cart.data.model.CartRowModel"/>
    </data>
    <FrameLayout android:layout_height="wrap_content"
        android:layout_width="match_parent" style="@style/groupStylecornerRadius">
        <include android:id="@+id/included"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" layout="@layout/component_order_details"/>
        <FrameLayout android:layout_gravity="bottom|end"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_16px"
            android:layout_marginEnd="@dimen/_48px"
            android:layout_marginTop="@dimen/_64px"
            android:layout_width="wrap_content" style="@style/groupStylecornerRadius">
            <TextView android:id="@+id/txt"
                android:layout_gravity="center"
                android:layout_height="match_parent"
                android:layout_width="match_parent" style="@style/textStyleregular0"/>
        </FrameLayout>
    </FrameLayout>
</layout>
